<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".AgendaDeGastos">

    <TextView
        android:id="@+id/textView"
        android:layout_width="360dp"
        android:layout_height="54dp"
        android:layout_marginTop="24dp"
        android:text="@string/textview"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="360dp"
        android:layout_height="44dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:hint="Busca la categoria"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="12dp"
        android:backgroundTint="#08AE9E"
        android:text="Regresar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editText" />

    <Button
        android:id="@+id/button2"
        android:layout_width="226dp"
        android:layout_height="47dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="32dp"
        android:backgroundTint="#08AE9E"
        android:text="Agregar Categoria"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/editText" />

    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton20"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconovideojuegos"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton21"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconoviaje"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton22"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconouber"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton23"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconotransporte"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton24"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconotarjetas"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton25"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconospotify"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton26"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconoropa"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton27"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconorappi"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton28"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/icononetflix"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton29"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconomercado"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton30"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconoluz"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton31"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconogas"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton32"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconofiesta"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton33"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconodatosmovil"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton34"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconocomida"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal">

                <ImageButton
                    android:id="@+id/imageButton35"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconocombustible"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton36"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/iconoagua"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />

                <ImageButton
                    android:id="@+id/imageButton37"
                    android:layout_width="125dp"
                    android:layout_height="112dp"
                    android:scaleType="centerCrop"
                    app:srcCompat="@drawable/pantalla5"
                    tools:layout_editor_absoluteX="270dp"
                    tools:layout_editor_absoluteY="0dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>